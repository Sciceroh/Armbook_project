class user:
    def __init__(self, senha, nome, email, idade):
        self.senha = senha
        self.nome = nome
        self.email = email
        self.idade = idade

    def set_email(self,email):
        self.email = email

    def set_senha(self,senha):
        self.senha = senha

    def set_nome(self, nome):
        self.nome = nome

    def set_idade(self, idade):
        self.idade = idade

