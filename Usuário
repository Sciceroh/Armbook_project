import * from Armbook.py


class user(Armbook):
  def __init__(self, senha nome, email, idade):
    self.senha = senha
    self.nome = nome
    self.email = email
    self.idade = idade

  def __init__(self, nome, email, idade):
    super(user, self).__init__(nome, email, idade)
    self._user = user

  def setuser(self, i):
    return self._user

  def set_nome(self, nome):
    if len(nome) > 0:
      self.nome = nome

  def set_idade(self, idade):
    if idade > 0:
      self.idade = idade

  def set_email(self,email):
    if len(nome) > 0:
      self.email = email
